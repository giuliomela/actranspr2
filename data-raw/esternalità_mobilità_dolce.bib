  author = {Mueller, Natalie and {Rojas-Rueda}, David and {Cole-Hunter}, Tom and {de Nazelle}, Audrey and Dons, Evi and Gerike, Regine and G{\"o}tschi, Thomas and Int Panis, Luc and Kahlmeier, Sonja and Nieuwenhuijsen, Mark},
