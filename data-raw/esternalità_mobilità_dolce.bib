@article{uijtdewilligen2022,
  title = {How Does Hourly Variation in Exposure to Cyclists and Motorised Vehicles Affect Cyclist Safety? {{A}} Case Study from a {{Dutch}} Cycling Capital},
  shorttitle = {How Does Hourly Variation in Exposure to Cyclists and Motorised Vehicles Affect Cyclist Safety?},
  author = {Uijtdewilligen, Teun and Ulak, Mehmet Baran and Wijlhuizen, Gert Jan and Bijleveld, Frits and Dijkstra, Atze and Geurs, Karst T.},
  year = {2022},
