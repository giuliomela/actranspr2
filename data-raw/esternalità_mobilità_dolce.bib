@article{guo2021,
  title = {Using Heart Rate to Estimate the Minute Ventilation and Inhaled Load of Air Pollutants},
  author = {Guo, Qian and Zhao, Yuchen and Shao, Jing and Cao, Suzhen and Wang, Qirong and Wu, Wei and Duan, Xiaoli},
