  title = {Physical Activity of Electric Bicycle Users Compared to Conventional Bicycle Users and Non-Cyclists: {{Insights}} Based on Health and Transport Data from an Online Survey in Seven {{European}} Cities},
  author = {Castro, Alberto and {Gaupp-Berghausen}, Mailin and Dons, Evi and Standaert, Arnout and Laeremans, Michelle and Clark, Anna and {Anaya-Boig}, Esther and {Cole-Hunter}, Tom and {Avila-Palencia}, Ione and {Rojas-Rueda}, David and Nieuwenhuijsen, Mark and Gerike, Regine and Panis, Luc Int and {de Nazelle}, Audrey and Brand, Christian and Raser, Elisabeth and Kahlmeier, Sonja and G{\"o}tschi, Thomas},
