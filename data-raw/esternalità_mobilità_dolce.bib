  journal = {arXiv:1706.00646 [cs]},
  eprint = {1706.00646},
  primaryclass = {cs},
  urldate = {2021-05-27},
  abstract = {The effect of transport-related pollution on human health is fast becoming recognized as a major issue in cities worldwide. Cyclists, in particular, face great risks, as they typically are most exposed to tail-pipe emissions. Three avenues are being explored worldwide in the fight against urban pollution: (i) outright bans on polluting vehicles and embracing zero tailpipe emission vehicles; (ii) measuring air-quality as a means to better informing citizens of zones of higher pollution; and (iii) developing smart mobility devices that seek to minimize the effect of polluting devices on citizens as they transport goods and individuals in our cities. Following this latter direction, in this paper we present a new way to protect cyclists from the effect of urban pollution. Namely, by exploiting the actuation possibilities afforded by pedelecs or e-bikes (electric bikes), we design a cyber-physical system that mitigates the effect of urban pollution by indirectly controlling the minute ventilation (volume of air inhaled per minute) of cyclists in polluted areas. Results from a real device are presented to illustrate the efficacy of our system.},
  archiveprefix = {arXiv},
  langid = {english},
  keywords = {Electrical Engineering and Systems Science - Systems and Control},
  file = {C:\Users\mela\Zotero\storage\P4RCI2YJ\Sweeney et al. - 2018 - A context-aware e-bike system to reduce pollution .pdf}
}

@article{Taddei2015,
