@article{sweeney2018,
  title = {A Context-Aware e-Bike System to Reduce Pollution Inhalation While Cycling},
  author = {Sweeney, Shaun and {Ordonez-Hurtado}, Rodrigo and Pilla, Francesco and Russo, Giovanni and Timoney, David and Shorten, Robert},
  year = {2018},
