@article{sommar2022,
  title = {Overall Health Impacts of a Potential Increase in Cycle Commuting in {{Stockholm}}, {{Sweden}}},
  author = {Sommar, Johan Nilsson and Johansson, Christer and L{\"o}venheim, Boel and Schantz, Peter and Markstedt, Anders and Str{\"o}mgren, Magnus and Stigson, Helena and Forsberg, Bertil},
